{
  "name": "tv-shows",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "npm run test:unit && npm run test:component",
    "test:unit": "vitest run --config vitest.config.unit.ts",
    "test:component": "vitest run --config vitest.config.component.ts",
    "test:watch": "vitest --config vitest.config.unit.ts",
    "test:watch:component": "vitest --config vitest.config.component.ts",
    "test:coverage": "vitest run --coverage --config vitest.config.unit.ts && vitest run --coverage --config vitest.config.component.ts"
  },
  "dependencies": {
    "@vueuse/core": "^14.1.0",
    "@vueuse/nuxt": "^14.1.0",
    "nuxt": "^4.2.2",
    "sass": "^1.97.0",
    "vue": "^3.5.25",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@nuxt/test-utils": "^3.19.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "happy-dom": "^17.6.3",
    "jiti": "^2.6.1",
    "typescript-eslint": "^8.50.0",
    "vitest": "^3.2.4"
  }
}
